if not _G.KEY then
    game.Players.LocalPlayer:Kick("Key inválida o no proporcionada")
    return
end

local Http = game:GetService("HttpService")
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()

local req = http_request or request or syn.request
if not req then
    game.Players.LocalPlayer:Kick("Executor no compatible")
    return
end

local res = req({
    Url = "https://jeje-1.onrender.com/validate",
    Method = "POST",
    Headers = {["Content-Type"] = "application/json"},
    Body = Http:JSONEncode({
        key = _G.KEY,
        hwid = hwid
    })
})

local data = Http:JSONDecode(res.Body)

if not data.valid then
    if data.reason == "EXPIRED" then
        game.Players.LocalPlayer:Kick("Tu key se venció")
    elseif data.reason == "HWID_LOCKED" then
        game.Players.LocalPlayer:Kick("HWID no autorizado")
    else
        game.Players.LocalPlayer:Kick("Key inválida")
    end
    return
end

loadstring(game:HttpGet(data.raw))()
